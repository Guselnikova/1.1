#include <stdio.h>

int is_simple (int number) 
{
  if (number < 2)
  {
    return 0;
  }
  for (int i = 2; i < number; i++)
  {
    if (number % i == 0)
    {
      return 0;
    }
  }
  return 1;
}

int main()
{
  int num;
  scanf("%d", &num);
  int chek = is_simple (num);
  printf("%d", chek);
  return 0;
}
